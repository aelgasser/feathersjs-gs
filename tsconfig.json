{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "sourceMap": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "removeComments": false,
        "noImplicitAny": false
    },
    "exclude": [
        "node_modules",
        "typings/express",
        "typings/mime",
        "typings/mongodb",
        "typings/node",
        "typings/rx",
        "typings/serve-static",
        "typings/socket.io",
        "typings/socket.io-client",
        "custom-typings/body-parser"
    ]
}